{
  "name": "codekite",
  "version": "1.0.0",
  "description": "Foundational JS project baseline",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel secure dev-srv lint:watch start-mockapi",
    "build": "webpack --config=./config/webpack.config.prod.js",
    "watch": "webpack --watch --config=./config/webpack.config.js",
    "secure": "npm audit",
    "ngrok": "./ngrok http -auth='someuser:somepass' 80",
    "dev-srv": "webpack-dev-server --config=./config/webpack.config.js",
    "lint": "esw webpack.config.*, src",
    "lint:watch": "npm run lint -- --watch",
    "test": "ava ./src/*.test.js ./src/components/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "test:cachebust": "ava --reset-cache",
    "gen-mockapi-data": "node ./src/api/generateMockData.js",
    "prestart-mockapi": "npm run gen-mockapi-data",
    "start-mockapi": "json-server --watch ./src/api/db.json --port 3001"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ]
      }
    },
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "ajv": "^6.5.2",
    "ava": "1.0.0-beta.6",
    "babel-loader": "^8.0.0-beta.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.10",
    "cssnano": "^4.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-ava": "^5.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^3.1.5",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.12.0",
    "json-schema-faker": "^0.5.0-rc15",
    "json-server": "^0.14.0",
    "mini-css-extract-plugin": "^0.4.1",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-initial": "^2.0.0",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react-css-modules": "^4.7.4",
    "style-loader": "^0.20.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "@babel/register": "^7.0.0-beta.56",
    "ajv-keywords": "^3.2.0",
    "hoek": "^5.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
  "author": "Cody Welsh <codyw@protonmail.com>",
  "license": "GPL-3.0"
}
