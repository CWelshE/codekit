{
  "name": "cckit",
  "version": "1.0.0",
  "description": "Foundational JS project baseline",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel secure dev-srv lint:watch start-mockapi",
    "watch": "webpack --watch --config=./config/webpack.config.js",
    "secure": "npm audit",
    "ngrok": "./ngrok http -auth='someuser:somepass' 80",
    "dev-srv": "webpack-dev-server --config=./config/webpack.config.js",
    "lint": "esw webpack.config.*, src",
    "lint:watch": "npm run lint -- --watch",
    "test": "ava ./src/*.tests.js",
    "test:watch": "npm test -- --watch",
    "gen-mockapi-data": "node ./src/api/generateMockData.js",
    "prestart-mockapi": "npm run gen-mockapi-data",
    "start-mockapi": "json-server --watch ./src/api/db.json --port 3001"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    }
  },
  "devDependencies": {
    "ajv": "^6.5.1",
    "ava": "1.0.0-beta.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "cssnano": "^4.0.0-rc.2",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^3.1.5",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "json-schema-faker": "^0.5.0-rc15",
    "json-server": "^0.14.0",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "prop-types": "^15.6.2",
    "react-css-modules": "^4.7.2",
    "style-loader": "^0.20.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "ajv-keywords": "^3.2.0",
    "hoek": "^5.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redux": "^4.0.0"
  },
  "author": "Cody Welsh <codyw@protonmail.com>",
  "license": "GPL-3.0"
}
